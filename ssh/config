Host *
    ControlMaster auto
    ControlPath ~/.ssh/master-%l-%r@%h:%p
    ControlPersist 300

    ServerAliveInterval 30
    ServerAliveCountMax 3

    ForwardAgent yes

    ForwardX11 yes
    ForwardX11Trusted yes

    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
